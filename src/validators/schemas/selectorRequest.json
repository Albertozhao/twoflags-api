{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "maxLength": 60
    },
    "op": {
      "type": "string",
      "enum": ["push", "pop", "shift", "unshift"]
    },
    "label": {
      "type": "string",
      "maxLength": 100
    },
    "value": {
      "type": "string",
      "maxLength": 100
    }
  },
  "additionalProperties": false
}
